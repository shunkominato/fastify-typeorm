      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.40"
   },
   "devDependencies": {
      "@tsconfig/node14": "^1.0.1",
      "@types/node": "^16.11.7",
      "@typescript-eslint/eslint-plugin": "^5.4.0",
      "@typescript-eslint/parser": "^5.4.0",
      "eslint": "^8.2.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.25.3",
      "jest": "^27.4.3",
      "prettier": "^2.4.1",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.1.8",
      "typescript": "^4.4.4"
